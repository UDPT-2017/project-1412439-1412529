<div id="page-wrapper">
	<div class="main-page">
		<div class="tables">
			<h3 class="title1">Danh Sách</h3>
			<h4>Danh Sách đơn đặt hàng</h4>
			<div class="panel-body widget-shadow" id="pagination_data">
				<table class='table table-bordered'>
					<thead>
						<tr>
						  <th>id</th>
						  <th>Username</th>
						  <th>ID Product</th>
						  <th>Price</th>
						  <th>Date order</th>


						</tr>
					</thead>
					<tbody>
						{{#each orders}}
						<tr>
						  <td>{{id}}</td>
						  <td>{{username}}</td>
						  <td>{{id_product}}</td>
						  <td>{{price}}</td>
						  <td>{{date}}</td>
						  
						</tr>
						{{/each}}
					</tbody>
				</table>

				<!--Pagination -->


			</div>
		</div>
	</div>
</div>
